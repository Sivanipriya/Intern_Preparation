string prefix_len(vector<string> strs) {

    string c="";
   
    sort(strs.begin(),strs.end());
        int n=strs.size()-1;
        for(long unsigned int i=0;i<strs[0].size();i++)
        {
            if(strs[0][i]==strs[n][i])
            {               
             c+=strs[0][i];
            }
            else
                break;
        }
    return c;
    
}
